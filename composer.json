{
    "name": "thulisoft/laravel-mongodb-passport",
    "description": "A package to allow laravel 8 /passport use with jenssegers/laravel-mongodb",
    "homepage": "https://github.com/thulisoft/laravel-mongodb-passport",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel-mongodb",
        "laravel-mongodb-passport",
        "laravel-passport",
        "mongodb",
        "passport",
        "mongo-passport"
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^8.0",
        "jenssegers/mongodb": "^3.8",
        "laravel/passport": "^10.1"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "MongoPassport\\Mongodb\\": "src"
        }
    },
    "authors": [{
        "name": "Thulisoft",
        "email": "thulisoft.info@gmail.com",
        "role": "Support"
    }],
    "extra": {
        "laravel": {
            "providers": [
                "MongoPassport\\Mongodb\\MongodbPassportServiceProvider"
            ]
        }
    }
}